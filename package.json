{
  "name": "dockument",
  "version": "1.0.0",
  "description": "A full stack document management system, complete with roles and privileges.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout 90000",
    "start:dev": "nodemon --exec babel-node app.js",
    "test:db:migrate": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "test:db:seed": "NODE_ENV=test sequelize db:seed:all",
    "start:test:db": "npm run test:db:migrate && npm run test:db:seed",
    "server-test": "npm run start:test:db && NODE_ENV=test nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/tests/**/*.js --timeout 90000",
    "dev:db:seed": "sequelize db:seed:all",
    "dev:db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "dev-server": "npm run dev:db:migrate && npm run dev:db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baasbank/dockument.git"
  },
  "keywords": [
    "Express",
    "Postgresql"
  ],
  "author": "Baasbank Akinmuleya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baasbank/dockument/issues"
  },
  "homepage": "https://github.com/baasbank/dockument#readme",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.2.1",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^1.9.4",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.6.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "node-mocks-http": "^1.6.4",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3"
  }
}
